	Journal = {Biotechnol J},
	Keywords = {Cell Movement, Age Factors, Plasmodium berghei, Green Fluorescent Proteins, Image Processing: Computer-Assisted, Anopheles, Microscopy: Fluorescence, Sporozoites, Salivary Glands, Pattern Recognition: Automated, Animals, Temperature, User-Computer Interface},
	Language = {eng},
