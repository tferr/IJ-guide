	Keywords = {Microscopy: Video, Angiogenesis Inhibitors, Vascular Endothelial Growth Factor A, Fibroblast Growth Factor 2, Neovascularization: Physiologic, Zebrafish, Software, Animals},
	Language = {eng},
	Month = {Sep},
	Number = {3},
	Pages = {239--43},
	Pmid = {19566407},
