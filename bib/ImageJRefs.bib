	Doi = {10.1002/cyto.a.20747},
	Journal = {Cytometry A},
	Keywords = {Image Cytometry, Software, Pattern Recognition: Automated, Fluorescence Recovery After Photobleaching, Cell Line: Tumor, Fluorescence Resonance Energy Transfer, Microscopy: Confocal, Humans},
	Language = {eng},
