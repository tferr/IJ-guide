	Doi = {10.1007/s12031-008-9129-8},
	Journal = {J Mol Neurosci},
	Keywords = {Paclitaxel, Fluorescent Dyes, Pheochromocytoma, Rats, PC12 Cells, Cell Line: Tumor, Mitochondria, Antineoplastic Agents: Phytogenic, Adrenal Gland Neoplasms, Animals},
	Language = {eng},
	Month = {Mar},
	Number = {3},
	Pages = {254--62},
	Pmid = {18636346},
	Rating = {0},
	Title = {{A novel method for analyzing mitochondrial movement: inhibition by paclitaxel in a pheochromocytoma cell model}},
	Volume = {37},
