	Journal = {Int J Radiat Oncol},
	Keywords = {Lung, Dose-Escalation, Radiotherapy, Organ Motion, Hepatobiliary Cancer, Liver Metastases, Movement, Cine-Mri, Computed-Tomography, Respiratory Organ Motion, Real-Time Tumor, Tracking, Position},
	Language = {English},
