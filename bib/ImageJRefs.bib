	Journal = {Cornea},
	Keywords = {Fuchs' Endothelial Dystrophy, Photography, Disease Progression, Image Processing: Computer-Assisted, Humans, Diagnostic Techniques: Ophthalmological},
	Language = {eng},
	Month = {May},
	Number = {4},
