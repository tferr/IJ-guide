	Keywords = {Humans, Adaptation: Physiological, Pliability, Child, Male, Absorptiometry: Photon, Biomechanics, Bone Development, Tibia, Tomography: X-Ray Computed, Time Factors, Bone Density, Motor Activity, Linear Models},
	Language = {eng},
