	Journal = {Microsc. Res. Tech.},
	Keywords = {Microscopy: Confocal, Receptors: G-Protein-Coupled, Software, Fluorescent Dyes, Arrestins, Image Processing: Computer-Assisted, Fluorescence Resonance Energy Transfer, Humans},
	Language = {eng},
