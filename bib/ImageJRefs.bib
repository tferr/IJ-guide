	Doi = {10.1186/1472-6750-9-59},
	Journal = {BMC Biotechnol},
	Keywords = {Cell Line, Embryonic Stem Cells, Software, Reverse Transcriptase Polymerase Chain Reaction, Mice, Sensitivity and Specificity, Animals, Chromatin Immunoprecipitation, Humans},
	Language = {eng},
	Month = {Jan},
	Pages = {59},
