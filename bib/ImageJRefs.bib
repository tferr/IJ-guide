	Keywords = {Luciferases, Hela Cells, Cell-Free System, Protein Processing: Post-Translational, Software, Transfection, Transcription: Genetic, I-kappa B Kinase, Genes: Reporter, Recombinant Fusion Proteins, Color, Diagnostic Imaging, Humans, Luminescence},
