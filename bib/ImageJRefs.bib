	Doi = {10.1016/j.jvoice.2007.07.002},
	Journal = {J Voice},
	Keywords = {Laryngeal Mucosa, Vocal Cords, Image Processing: Computer-Assisted, Reproducibility of Results, Dogs, Animals, Laryngeal Diseases},
	Language = {eng},
	Month = {Jan},
	Number = {1},
