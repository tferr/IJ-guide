	Doi = {10.1111/j.1600-0846.2009.00393.x},
	Journal = {Skin Res Technol},
	Keywords = {Adult, Microscopy: Confocal, Female, Humans, Middle Aged, Aged: 80 and over, Forearm, Male, Psoriasis, Reproducibility of Results, Microscopic Angioscopy, Young Adult, Sex Characteristics, Aged, Dermis},
	Language = {eng},
