	Keywords = {Programming Languages, Artifacts, Artificial Intelligence, Protein Interaction Mapping, Software, Pattern Recognition: Automated, Fluorescence Recovery After Photobleaching, Sensitivity and Specificity, Reproducibility of Results, Algorithms},
	Language = {eng},
