\begin_layout Plain Layout

/* These two macros loop through the tools listed in an array using "F1"
 and "F2" as keyboard shortcuts (forward and reverse cycling).
 */
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

 var index;
\end_layout

\begin_layout Plain Layout

 var tools = newArray("rectangle", "roundrect", "oval", "ellipse", "brush",
 "polygon", "freehand", "line", "freeline", "polyline", "arrow", "wand",
 "dropper", "angle", "point", "multipoint", "text");
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

 macro "Cycle Tools Fwd [F1]" {
\end_layout

\begin_layout Plain Layout

  setTool(tools[index++]);
\end_layout

\begin_layout Plain Layout

  if (index==tools.length) index = 0; 
\end_layout

\begin_layout Plain Layout

 } 
\end_layout

\begin_layout Plain Layout

   
\end_layout

\begin_layout Plain Layout

 macro "Cycle Tools Rwd [F2]" {
\end_layout

\begin_layout Plain Layout

  if (index<0) index = tools.length-1;
\end_layout

\begin_layout Plain Layout

  setTool(tools[index--]);
\end_layout

\begin_layout Plain Layout

 }
\end_layout

\end_inset


\end_layout

